["html:div", {"class":"DotaTooltip DotaFont DotaTooltip_hero"},
  ["html:div", {"class":"heroName","linked-text":"[[dname]]"}],
  ["html:div", {"class":"heroRoles"},
    ["html:span", {"class":"heroAttackCability","linked-text":"[[dac]]"}],
    ["html:span", {"linked-text":"[[droles]]"}]
  ],
  ["html:div", {"class":"heroPortraitStats"},
    ["html:div", {"class":"heroPrimaryPortraitHolder"},
      ["html:img", {"class":"heroPrimaryPortraitImg","linked-src":"https://cdn.steamstatic.com/apps/dota2/images/heroes/[[objname]]_vert.jpg","src":""}],
      ["html:img", {"class":"heroPrimaryPortraitOverlay","src":"https://cdn.steamstatic.com/apps/dota2/images/heropedia/heroprimaryportrait_overlay.png"}]
    ],
    ["html:div", {"class":"heroStats"},
      ["html:div", {"class":"StatVal"},
        ["html:img", {"class":"Attr","contenteditable":"","title":"Intelligence","src":"https://cdn.steamstatic.com/apps/dota2/images/heropedia/overviewicon_int.png"}],
        ["html:img", {"contenteditable":"","linked-id":"overviewIcon_Primary_[[pa]]","src":"https://cdn.steamstatic.com/apps/dota2/images/heropedia/overviewicon_primary.png","class":"Attr overviewIcon_Primary_int"}],
        ["html:span", {"class":"StatText","linked-text":"[[attribs.int.b]]+[[attribs.int.g]]"}]
      ],
      ["html:div", {"class":"StatVal"},
        ["html:img", {"class":"Attr","contenteditable":"","title":"Intelligence","src":"https://cdn.steamstatic.com/apps/dota2/images/heropedia/overviewicon_str.png"}],
        ["html:img", {"contenteditable":"","linked-id":"overviewIcon_Primary_[[pa]]","src":"https://cdn.steamstatic.com/apps/dota2/images/heropedia/overviewicon_primary.png","class":"Attr overviewIcon_Primary_str"}],
        ["html:span", {"class":"StatText","linked-text":"[[attribs.str.b]]+[[attribs.str.g]]"}]
      ],
      ["html:div", {"class":"StatVal"},
        ["html:img", {"class":"Attr","title":"Intelligence","src":"https://cdn.steamstatic.com/apps/dota2/images/heropedia/overviewicon_agi.png"}],
        ["html:img", {"linked-id":"overviewIcon_Primary_[[pa]]","src":"https://cdn.steamstatic.com/apps/dota2/images/heropedia/overviewicon_primary.png","class":"Attr overviewIcon_Primary_agi"}],
        ["html:span", {"class":"StatText","linked-text":"[[attribs.agi.b]]+[[attribs.agi.g]]"}]
      ]
    ],
    ["html:div", {"class":"heroStats"},
      ["html:div", {"class":"StatVal"},
        ["html:img", {"class":"BaseStat","title":"Damage","src":"https://cdn.steamstatic.com/apps/dota2/images/heropedia/overviewicon_attack_2.png"}],
        ["html:span", {"class":"StatText","linked-text":"[[attribs.dmg.min]]-[[attribs.dmg.max]]"}]
      ],
      ["html:div", {"class":"StatVal"},
        ["html:img", {"class":"BaseStat","title":"Movespeed","src":"https://cdn.steamstatic.com/apps/dota2/images/heropedia/overviewicon_speed_2.png"}],
        ["html:span", {"class":"StatText","linked-text":"[[attribs.ms]]"}]
      ],
      ["html:div", {"class":"StatVal"},
        ["html:img", {"class":"BaseStat","title":"Defense","src":"https://cdn.steamstatic.com/apps/dota2/images/heropedia/overviewicon_defense_2.png"}],
        ["html:span", {"class":"StatText","linked-text":"[[attribs.armor]]"}]
      ]
    ]
  ]
]
